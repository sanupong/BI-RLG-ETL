        int c = s.Length;
        if (c == 3)
        {
            Row.H1 = s[0];
            Row.H2 = s[1];
            Row.H3 = s[2];
        }
        else if (c == 2)
        {
            Row.H1 = s[0];
            Row.H2 = s[1];
            Row.H3 = "N/A";
            Row.ItemCategory = s[0] + "-" + s[1] + "-N/A";
        }
        else
        {
            Row.H1 = s[0];
