    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{32D997C6-1FA6-49A9-8406-7EC7524DF4B5}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;Select Cast(a.CODE as NVarchar(255)) as CODE, Cast(a.DESCRIPTION as NVarchar(255)) as DESCRIPTION, a.Start_Date_Active, a.End_Date_Active, a.Creation_Date, a.Last_Update_Date, Cast('' as NVarchar(1)) as Attribute1, Cast('' as NVarchar(1)) as Attribute2 &#xA;, b.Mapping, b.ProjectGroup, b.Project, b.Running &#xA;From Segment5 a &#xA;Left Join Segment5_Mapping b&#xA; On Cast(a.CODE as NVarchar(255)) = b.Code&#xA;Where Convert(varchar(8), a.LAST_UPDATE_DATE, 112) Between '&quot; + @[User::ETLStartDate] + &quot;' And  '&quot; + @[User::ETLEndDate] + &quot;' Or Convert(varchar(8), a.CREATION_DATE, 112) Between '&quot; + @[User::ETLStartDate] + &quot;' And  '&quot; + @[User::ETLEndDate] + &quot;' Order by a.CODE;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQL">
      <DTS:VariableValue
        DTS:DataType="8">Select Cast(a.CODE as NVarchar(255)) as CODE, Cast(a.DESCRIPTION as NVarchar(255)) as DESCRIPTION, a.Start_Date_Active, a.End_Date_Active, a.Creation_Date, a.Last_Update_Date, Cast('' as NVarchar(1)) as Attribute1, Cast('' as NVarchar(1)) as Attribute2 
, b.Mapping, b.ProjectGroup, b.Project, b.Running 
From Segment5 a 
Left Join Segment5_Mapping b
 On Cast(a.CODE as NVarchar(255)) = b.Code
Where Convert(varchar(8), a.LAST_UPDATE_DATE, 112) Between '20130101' And  '20131231' Or Convert(varchar(8), a.CREATION_DATE, 112) Between '20130101' And  '20131231' Order by a.CODE;</DTS:VariableValue>
    </DTS:Variable>
